{% extends 'base.html.twig' %}

{% block title %}
	Contact
{% endblock %}

{% block body %}


	{{ form_start(form) }}

	{{ form_row(form.mail) }}
	{{ form_row(form.name) }}
	{{ form_row(form.objet) }}

	{{ form_row(form.message) }}

	{{ form_widget(form) }}
	<button type="submit" class="classes">Envoyer</button>

	{{ form_end(form) }}

	{% if {state} %}
		<div>
			<p>{{message}}</p>
		</div>

	{% endif %}


{% endblock %}
